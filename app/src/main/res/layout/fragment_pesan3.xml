<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             xmlns:app="http://schemas.android.com/apk/res-auto"
             tools:context=".HomeOrder.Pesan3Fragment"
             android:background="@color/colorPrimaryDark">

    <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        <TextView
                android:text="Dengan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/white"
                android:fontFamily="@font/telex"
                android:id="@+id/textView30"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="16dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="88dp"
                app:layout_constraintTop_toTopOf="parent"/>

        <TextView
                android:text="mba fitri"
                android:layout_width="wrap_content"
                android:layout_height="17dp"
                android:textColor="@color/gold"
                android:fontFamily="@font/telex"
                android:id="@+id/tv_nama_pemesan"
                app:layout_constraintStart_toEndOf="@+id/textView30"
                android:layout_marginLeft="6dp"
                android:layout_marginStart="6dp"
                android:layout_marginBottom="53dp"
                app:layout_constraintBottom_toTopOf="@+id/textView31"/>

        <TextView
                android:text="Lokasi pemesanan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/white"
                android:fontFamily="@font/telex"
                tools:layout_editor_absoluteX="146dp"
                android:id="@+id/textView29"
                android:layout_marginBottom="16dp"
                app:layout_constraintBottom_toTopOf="@+id/button3"/>

        <TextView
                android:text="ALAMAT"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/gold"
                android:fontFamily="@font/telex"
                android:id="@+id/textView31"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="16dp"
                android:layout_marginStart="16dp"
                android:layout_marginBottom="18dp"
                app:layout_constraintBottom_toTopOf="@+id/linearLayout"/>

        <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:id="@+id/linearLayout"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginBottom="38dp"
                app:layout_constraintBottom_toTopOf="@+id/textView32">
        </LinearLayout>

        <TextView
                android:text="PATOKAN"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/telex"
                android:textColor="@color/gold"
                android:id="@+id/textView32"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="16dp"
                android:layout_marginStart="16dp"
                android:layout_marginBottom="14dp"
                app:layout_constraintBottom_toTopOf="@+id/linearLayout2"/>

        <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:id="@+id/linearLayout2"
                tools:layout_editor_absoluteY="325dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent">
        </LinearLayout>

        <Button
                android:layout_width="170dp"
                android:layout_height="60dp"
                tools:layout_editor_absoluteY="552dp"
                android:background="@drawable/btn_back"
                android:id="@+id/button3"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="16dp"
                android:layout_marginStart="16dp"/>

        <Button
                android:layout_width="170dp"
                android:layout_height="60dp"
                tools:layout_editor_absoluteY="552dp"
                android:background="@drawable/btn_next"
                android:id="@+id/button4"
                android:layout_marginEnd="16dp"
                android:layout_marginRight="16dp"
                app:layout_constraintEnd_toEndOf="parent"/>


    </android.support.constraint.ConstraintLayout>

</FrameLayout>